<config>
    <input>
        <jar in="newService-0.0.1-SNAPSHOT.war" out="newService_allatori-0.0.1-SNAPSHOT.war"/>
    </input>
    <ignore-classes>
        <class template="class *springframework*"/>
        <class template="class *shardingjdbc*"/>
        <class template="class *jni*"/>
        <class template="class *alibaba*"/>
        <class template="class *persistence*"/>
        <!-- 排除如下两个包下的类-->
        <class template="class com.ky.newService.NewServiceApplication"/>
        <class template="class com.ky.newService.service*"/>
        <class template="class com.ky.newService.entity*"/>
        <class template="class com.ky.newService.controller*"/>
    </ignore-classes>
    <keep-names>
        <class access="protected+">
            <field access="protected+"/>
            <method access="protected+"/>
        </class>
    </keep-names>
    <property name="log-file" value="log.xml"/>
</config>